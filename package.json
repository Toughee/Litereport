{
  "name": "Litereport",
  "version": "1.4.0",
  "description": "Custom script for automatic Lighthouse reports in localhost projects",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "litereport": "lighthouse http://localhost:3000/ --output json --output csv --output html --output-path reports/Lite.html --view | lighthouse-ci http://localhost:3000/"
  },
