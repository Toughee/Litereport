  "bugs": {
    "url": "https://github.com/Toughee/Litereport/issues"
  },
  "homepage": "https://github.com/Toughee/Litereport#readme",
  "dependencies": {
    "handlebars": "^4.7.7",
    "kind-of": "^6.0.3",
    "lighthouse": "^6.0.0",
    "lighthouse-ci": "^1.10.4",
    "minimist": "^1.2.6",
    "node": "^12.11.1",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.11.0",
    "server": "^1.0.19",
